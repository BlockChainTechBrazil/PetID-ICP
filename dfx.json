{
  "version": 1,
  "canisters": {
    "hello_world": {
      "type": "motoko",
      "main": "src/hello_world/main.mo"
    },
    "petid_frontend": {
      "dependencies": [
        "hello_world"
      ],
      "frontend": {
        "entrypoint": "src/petid_frontend/src/index.html"
      },
      "source": [
        "src/petid_frontend/assets",
        "dist/petid_frontend/"
      ],
      "type": "assets"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://ic0.app"
      ],
      "type": "persistent"
    }
  }
}
