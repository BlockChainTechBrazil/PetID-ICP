{
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "name": "petid",
  "scripts": {
    "canisters:start": "dfx start --background || true",
    "canisters:stop": "dfx stop || true",
    "initial": "npm run canisters:stop && rm -rf .dfx && dfx start --clean --background && dfx deploy && dfx deps pull && dfx deps init && dfx deps deploy && ./update-env.sh && npm run start --workspace src/PetID_frontend",
    "dev": "npm run canisters:start && npm run start --workspace src/PetID_frontend",
    "build": "npm run build --workspaces --if-present",
    "prebuild": "npm run prebuild --workspaces --if-present",
    "pretest": "npm run prebuild --workspaces --if-present",
    "start": "npm start --workspaces --if-present",
    "test": "npm test --workspaces --if-present",
    "test:backend": "vitest run tests/backend.test.js",
    "test:frontend": "vitest run tests/frontend.test.js", 
    "test:integration": "vitest run tests/integration.test.js",
    "test:all": "vitest run tests/",
    "test:watch": "vitest tests/",
    "test:ui": "vitest --ui tests/",
    "test:coverage": "vitest run --coverage tests/"
  },
  "type": "module",
  "workspaces": [
    "src/PetID_frontend"
  ],
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "c8": "^10.1.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "react-icons": "^5.5.0"
  }
}
